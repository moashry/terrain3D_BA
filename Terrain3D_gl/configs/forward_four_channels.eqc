server {
	config {
		appNode {
			pipe {
				window {
					viewport [ 560 200 800 800 ]
					channel {
						viewport [0 0 1 1]
						name "dest"
					}
 					channel {
 						viewport [0 0 0.25 1]
 						name "1"
 					}
					channel {
 						viewport [0.25 0 0.25 1]
						name "2"
					}
 					channel {
 						viewport [0.5 0 0.25 1]
 						name "3"
 					}
 					channel {
 						viewport [0.75 0 0.25 1]
 						name "4"
 					}
 				}
			}
		}
		compound {
			channel "dest"
			wall {
				bottom_left  [ -.414 -.414 -1 ]
				bottom_right [  .414 -.414 -1 ]
				top_left     [ -.414  .414 -1 ]
			}
			task [ CLEAR ]
			
			compound {
				channel "1"
 				viewport [0 0 0.25 1]
				task [ DRAW ]
			}
			compound {
				channel "2"
 				viewport [0.25 0 0.25 1]
				task [ DRAW ]
			}
			compound {
				channel "3"
 				viewport [0.5 0 0.25 1]
				task [ DRAW ]
			}
			compound {
				channel "4"
 				viewport [0.75 0 0.25 1]
				task [ DRAW ]
			}
		}
	}
}